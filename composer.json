{
    "name": "test-db-acle/db-fixture-dsl",
    "description": "An extendable DSL to put a database into a specified state",
    "type": "library",
    "require": {
        "php": ">8",
        "php-di/php-di": "^7.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vlucas/phpdotenv": "^5.6@dev"
    },
    "license": "BSD",
    "autoload": {
        "psr-4": {
            "TestDbAcle\\DbFixtureDsl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestDbAcleTests\\DbFixtureDsl\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Malte Riesch"
        }
    ],
    "minimum-stability": "dev"
}
